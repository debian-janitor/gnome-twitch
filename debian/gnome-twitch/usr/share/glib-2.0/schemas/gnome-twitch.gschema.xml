<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <enum id="com.gnome-twitch.GtTwitchStreamQuality">
    <value nick="source" value='0'/>
    <value nick="high" value='1'/>
    <value nick="medium" value='2'/>
    <value nick="low" value='3'/>
  </enum>
  <schema path="/com/gnome-twitch/app/" id="com.gnome-twitch.app">
    <key name="default-quality" enum="com.gnome-twitch.GtTwitchStreamQuality">
      <default>"source"</default>
      <summary>Default quality</summary>
      <description>Default quality selected when opening a stream</description>
    </key>
    <key name="volume" type="d">
      <range min="0.0" max="1.0"/>
      <default>0.3</default>
    </key>
    <key name="window-height" type="i">
      <default>710</default>
    </key>
    <key name="window-width" type="i">
      <default>1050</default>
    </key>
    <key name="oauth-token" type="s">
      <default>''</default>
    </key>
    <key name="user-name" type="s">
      <default>''</default>
    </key>
  </schema>
</schemalist>
